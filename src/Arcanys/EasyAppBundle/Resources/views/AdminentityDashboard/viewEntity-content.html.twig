        <div class="input-head">
            {#<span><a href="#" class="edit-user" id="{{ entity.id }}"><i class="icons edit"></i>edit</a></span>#}
            <span><a href="#" class="remove-user" id="{{ entity.id }}"><i class="icons rem"></i>remove</a></span>
        </div>
        {#{{ dump(bankinfo) }}#}

