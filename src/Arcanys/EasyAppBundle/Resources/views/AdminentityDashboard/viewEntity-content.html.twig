                    <div class="forth">
                        <label for="textCity">City</label>
                        <input type="text" name="city" id="textCity" value="{% if bankinfo|length >= 1 %}{{ bankinfo[0].entityCity }} {% endif %}" placeholder="City" class="textCity validate[required]" />
                        <span class="city-error"></span>
                    </div>
                    <div class="forth">
                        <label for="textState">State</label>
                        <select name="state" id="textState" class="textState validate[required]">
                            <option value="">Select a state</option>
                            {% for key,list in state %}
