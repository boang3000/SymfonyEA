                        <label for="textBankAddress">Street Name</label>
                        <input type="text" name="bankaddress" id="textBankAddress" value="{% if bankinfo|length >= 1 %}{{ bankinfo[0].entityAddress }} {% endif %}" placeholder="Street Name" class="textBankAddress validate[required]" />
                        <span class="bankaddress-error"></span>
