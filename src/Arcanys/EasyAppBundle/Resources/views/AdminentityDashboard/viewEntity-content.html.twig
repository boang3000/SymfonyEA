                    <div class="half">
                        <label for="textBank">Bank</label>
                        <input type="text" name="bankname" id="textBank" value="{% if bankinfo|length >= 1 %}{{ bankinfo[0].bankName|trim(' ') }} {% endif %}" placeholder="Bank Name" class="textBank validate[required]" />
                    </div>
                    <div class="half">
                        <label for="textBankContact">Bank Contact</label>
                        <input type="text" name="bankcontact" id="textBankContact" value="{% if bankinfo|length >= 1 %}{{ bankinfo[0].bankContact }} {% endif %}" placeholder="Bank Name Contact" class="textBankContact validate[required]" />
                    </div>
                </div>
                <div class="fields">
                    <div class="half">
                        <label for="textBankPhoneNum">Bank Phone Number</label>
                        <input type="text" name="bankphonenum" id="textBankPhoneNum" value="{% if bankinfo|length >= 1 %}{{ bankinfo[0].entityContact }} {% endif %}" maxlength="10" placeholder="00000" class="textBankPhoneNum validate[required,custom[number],maxSize[12],minSize[1]]" />
