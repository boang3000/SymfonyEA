                <button type='button' data-id='1' data-value='{% if bankinfo|length >= 1 %}{{ bankinfo[0].entitybankId }} {% endif %}' class='exit-tabentity-btn exit-tabentity-btn-1'></button>
                <a href='#' class='tabentity-edit' data-id='1'>edit</a>
