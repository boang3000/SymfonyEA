                <div class="full">
                    <label for="textName">Name</label>
                    <input type="text" name="entityname" id="textName" value="{{ entity.entityName }}" placeholder="Entity Name" class="validate[required]" />
                    <span class="entityname-error field-error"></span>
                </div>
            </div>
        </form>
